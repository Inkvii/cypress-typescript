{
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier",
      "plugin:cypress/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    },
    "plugins": [
      "cypress"
    ],
    "rules": {
      "@typescript-eslint/no-inferrable-types": 0,
      "@typescript-eslint/no-unused-vars": "warn"
    },
    "ignorePatterns": [
      "env.d.ts"
    ]
  }